    "    value_labels = sr.value_labels()\n",
    "\n",
    "df = pd.read_stata(\n",
    "    filepath,\n",
    "    convert_categoricals=False,\n",
    ")\n",
    "\n",
    "for col in value_labels:\n",
    "    if col.lower() in df.columns:\n",
    "        df[col.lower()].replace(value_labels[col], inplace=True)\n",
    "\n",
    "df.head()"
   ]
